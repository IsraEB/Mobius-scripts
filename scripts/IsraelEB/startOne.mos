# If all the tracks are muted, only start the selected track but, all the tracks are synchronized

!name StartOne

call MuteAllV

variable t track
variable i 1

while i <= trackCount
    Track i

    set muteMode start

	Mute
	Mute

    if i == t
        Mute
    endif

    set muteMode continue
    set i i+1
next

Track t
